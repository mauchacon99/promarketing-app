{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,md,js}\"",
    "format:check": "prettier --check  \"**/*.{ts,tsx,md,js}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.5",
    "@turbo/gen": "^1.9.7",
    "eslint": "7.27.0",
    "eslint-config-custom": "0.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.5.1",
    "turbo": "^1.9.7"
  },
  "name": "skin-promarketing-app",
  "packageManager": "yarn@1.22.11",
  "workspaces": [
    "apps/*",
    "apps/docs/*",
    "apps/webs/*",
    "packages/**"
  ]
